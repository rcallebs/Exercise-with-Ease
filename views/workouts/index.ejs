<%- include('../partials/header') %>



<h2>Current Workout Schedule</h2>
<table>
  <thead>
    <tr>
      <th>Day</th>
      <th>Details</th>
    </tr>
  </thead>
  <tbody>
    <% workouts.forEach(w => { %>

    <tr>
      <td><%= (w.day) %></td>
      <td><a href="/workouts/<%= w._id %>">Full Workout Information</a></td>
    </tr>
    <% }) %>
  </tbody>
</table>

<h3>Workout Schedule as of <%= time %></h3>

<%- include('../partials/footer') %>
